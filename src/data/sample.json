    "fieldDescriptions": [
        {
            "field": "allowUserDisplayChanges",
            "description": "Allow user changes. Controls whether the display resolution can be overridden by users.",
            "knownValueDescriptions": [
                {
                    "value": "true",
                    "description": "Allow users to overwrite predefined display settings (recommended)."
                },
                {
                    "value": "false",
                    "description": "Do not allow user changes for predefined display settings."
                }
            ],
            "defaultValue": true,
            "name": "Allow user changes.",
            "fieldDescription": "Controls whether the display resolution can be overridden by users.",
            "fieldConstraints": {}
        },
        {
            "field": "externalUseNativeResolution",
            "description": "External resolution. If use custom resolution is selected, the custom resolution will be applied to all external monitors. If the resolution is not supported, it will revert to the built in resolution.",
            "knownValueDescriptions": [
                {
                    "value": "true",
                    "description": "Always use native resolution."
                },
                {
                    "value": "false",
                    "description": "Use custom resolution."
                }
            ],
            "defaultValue": true,
            "name": "External resolution.",
            "fieldDescription": "If use custom resolution is selected, the custom resolution will be applied to all external monitors. If the resolution is not supported, it will revert to the built in resolution.",
            "fieldConstraints": {}
        },
        {
            "field": "externalDisplayWidth",
            "description": "External display width (in pixels). Specifies the preferred width (in pixels) of any external monitor connected to a managed Chromebook.",
            "inputConstraint": "The long field should have value greater than or equal to zero.",
